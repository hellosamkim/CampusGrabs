%h1 Listing images

%table
  %thead
    %tr
      %th Listing
      %th colspan="3"
  %tbody
    - @images.each do |image|
      %tr
        %td
          = image.listing_id 
        %td
          = link_to 'Show', image 
        %td
          = link_to 'Edit', edit_image_path(image)
        %td
          = link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' }
%br

= link_to 'New Image', new_listing_image_path
