= render 'nav_1'
%div.breakline
= render 'nav_2'

.listings_wrapper
  %div.row
    - @listings.each do |listing|
      %div.col-md-4
        .each_listing
          %a{:href => listing_path(listing.id)}
            - listing.images[0..0].each_with_index do |image|
              .center-image
                = image_tag image.picture.url, :class => "thumbs"


